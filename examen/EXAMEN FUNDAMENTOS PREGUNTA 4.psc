Algoritmo sumarestamultiplicacionypotenciaJA
	Escribir "este programa suma resta multiplica y eleva a la potencia";
	Escribir "digite el primer numero";
	Leer a;
	Escribir "digite el segundo numero";
	Leer b;
	s<-a+b
	r<-a-b
	m<-a*b
	p<-a^b
	Escribir "el resultado de ",a,"+",b,"=",s,"";
	Escribir "el resultado de ",a,"-",b,"=",r,"";
	Escribir "el resultado de ",a,"*",b,"=",m,"";
	Escribir "el resultado de ",a,"^",b,"=",p,"";
	
	
FinAlgoritmo
